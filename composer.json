{
  "name": "tfrommen/unit-test-examples",
  "description": "Example code for my talks and workshops on unit testing.",
  "keywords": [
    "code",
    "es6",
    "javascript",
    "mock",
    "mocking",
    "patch",
    "php",
    "test",
    "testable",
    "testing",
    "unit",
    "wordpress",
    "wp"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Thorsten Frommen",
      "email": "info@tfrommen.de",
      "homepage": "https://tfrommen.de",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/tfrommen/unit-test-examples/issues",
    "source": "https://github.com/tfrommen/unit-test-examples"
  },
  "require": {
    "php": ">=7"
  },
  "require-dev": {
    "antecedent/patchwork": "^2.0.0",
    "brain/monkey": "^2.2.0",
    "mockery/mockery": "^1.0.0",
    "phpunit/phpunit": "^5.7.27"
  },
  "autoload": {
    "psr-4": {
      "tfrommen\\UnitTestExamples\\": "src/php"
    }
  },
  "autoload-dev": {
    "files": [
      "src/php/functions.php"
    ],
    "psr-4": {
      "tfrommen\\UnitTestExamples\\Tests\\": "tests/php"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}
